{
    "name": "@feature-framework/cli",
    "version": "1.0.18",
    "description": "Cli interface for feature framework",
    "scripts": {
        "watch": "tsc --watch",
        "check": "tsc",
        "clean": "rm -rf ./dist",
        "build": "npm run clean && npm install && tsc --noEmit false",
        "release": "npm run build && npm run lint && np --no-tests",
        "lint": "npx eslint ./src",
        "lint-fix": "npx eslint ./src --fix"
    },
    "bin": {
        "craft": "dist/src/index.js"
    },
    "keywords": [
        "Feature",
        "framework",
        "cli",
        "typescript"
    ],
    "repository": {
        "type": "git",
        "url": "git+ssh://git@github.com/deepslam/feature-framework-cli.git"
    },
    "author": {
        "name": "Dmitry Ivanov",
        "url": "https://github.com/deepslam/feature-framework-cli/",
        "email": "deepslam@gmail.com"
    },
    "license": "ISC",
    "dependencies": {
        "@feature-framework/core": "^0.5.2",
        "@ts-morph/bootstrap": "^0.6.0",
        "boxen": "^4.2.0",
        "chalk": "^4.1.0",
        "commander": "^6.1.0",
        "handlebars": "^4.7.6",
        "inquirer": "^7.3.3",
        "prettier": "^2.1.2",
        "ts-morph": "^8.1.0",
        "ts-node": "^9.0.0",
        "tsconfig-resolver": "^3.0.1",
        "typescript": "^4.0.2",
        "yargs": "^16.0.3"
    },
    "devDependencies": {
        "@types/chalk": "^2.2.0",
        "@types/inquirer": "^7.3.1",
        "@types/node": "^14.10.2",
        "@types/prettier": "^2.1.1",
        "@typescript-eslint/eslint-plugin": "^3.7.1",
        "@typescript-eslint/parser": "^3.7.1",
        "eslint": "^7.5.0",
        "eslint-config-airbnb": "^18.2.0",
        "eslint-config-prettier": "^6.12.0",
        "eslint-plugin-import": "^2.22.0",
        "eslint-plugin-jest": "^23.20.0",
        "eslint-plugin-jsx-a11y": "^6.3.1",
        "eslint-plugin-prettier": "^3.1.4"
    }
}
