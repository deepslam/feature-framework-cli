{
    "name": "@feature-framework/cli",
    "version": "1.0.0",
    "description": "Cli interface for feature framework",
    "scripts": {
        "watch": "tsc --watch",
        "check": "tsc",
        "clean": "rm -rf ./dist",
        "build": "npm run clean && tsc --noEmit false",
        "release": "npm run build && np",
        "test": "jest",
        "jest-watch": "jest --watch"
    },
    "bin": {
        "craft": "dist/src/index.js",
        "@feature-framework/cli": "dist/src/index.js"
    },
    "keywords": [
        "Feature",
        "framework",
        "cli",
        "typescript"
    ],
    "repository": {
        "type": "git",
        "url": "git+ssh://git@github.com/deepslam/feature-framework-cli.git"
    },
    "author": {
        "name": "Dmitry Ivanov",
        "url": "https://github.com/deepslam/feature-framework-cli/",
        "email": "deepslam@gmail.com"
    },
    "license": "ISC",
    "dependencies": {
        "@feature-framework/core": "^0.3.1",
        "boxen": "^4.2.0",
        "chalk": "^4.1.0",
        "commander": "^6.1.0",
        "handlebars": "^4.7.6",
        "inquirer": "^7.3.3",
        "ts-node": "^9.0.0",
        "typescript": "^4.0.2",
        "yargs": "^16.0.3"
    },
    "devDependencies": {
        "@types/node": "^14.10.2"
    }
