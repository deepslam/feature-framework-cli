    "url": "git+ssh://git@github.com/deepslam/feature-framework-cli.git"
  },
  "author": {
    "name": "Dmitry Ivanov",
    "url": "https://github.com/deepslam/feature-framework-cli/",
    "email": "deepslam@gmail.com"
  },
  "license": "ISC",
  "dependencies": {
    "@feature-framework/core": "^0.3.4",
    "@ts-morph/bootstrap": "^0.6.0",
    "boxen": "^4.2.0",
    "chalk": "^4.1.0",
    "commander": "^6.1.0",
    "handlebars": "^4.7.6",
    "inquirer": "^7.3.3",
    "ts-morph": "^8.1.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2",
    "yargs": "^16.0.3"
  },
  "devDependencies": {
